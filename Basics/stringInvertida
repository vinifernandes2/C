#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char entrada[30];
    //para limpeza do buffer de memória
    fflush(stdin);
    gets(entrada);
    //o laço for começa da última posição da string até a primeira
    //a função strlen retorna o tamanho da string
    for(int i=strlen(entrada)-1;i>=0;i--){
        printf("%c\n", entrada[i]);
    }

    return 0;
}
